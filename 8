#include<iostream> 

using namespace std;

int main()
{
	int value = 0;
	int N = 0;

	while (true)
	{
		cin >> N;
		if (1 <= N && N <= 1000000) {
			break;
		}
	}
	for (int i = 1; i <= N; i++)
	{
		int a = i;
		int sum = 0;
		int rang = 1;
		for(int i = 0; i<N; i++){
			if (a < 10){
				sum += a;
				break;
			}
			rang++;
			sum += a % 10;
			a /= 10;
			
		}
		if (sum % rang == 0)
			value++;
	}
	cout << value;
	return 0;
}
