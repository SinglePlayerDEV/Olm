#include <iostream>
#include <vector>
using namespace std;

int main()
{
    int str, stl;
    cin >> str >> stl;
    vector<vector<int>>arr(str, vector<int>(stl));

    for (int i = 0; i < str; i++) {
        for (int j = 0; j < stl; j++){
            cin >> arr[i][j];
        }
    }
    for (int i = 0; i < str; i++){
        for (int j = 0; j < stl; j++){
            cout << "\t" << arr[i][j];
        }
        cout << endl;
    }
    for (int i = 0; i < stl; i++) {
        int maxelement = arr[0][i];
        for (int j = 0; j < str; j++) {
            if (maxelement < arr[j][i])    {
                maxelement = arr[j][i];
            }
        }
        cout << maxelement;
    }
    return 0;
}
